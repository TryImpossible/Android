<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bynn.integration">

    <application
        android:allowBackup="true"
        android:icon="${app_icon}"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${ENVIRONMENT}" />

        <activity android:name=".activity.EBFirstActivity" />
        <activity android:name=".activity.EBSecondActivity" />
        <activity android:name=".activity.PhotoViewActivity" />
        <activity android:name=".activity.EBThridActivity"></activity>
    </application>

</manifest>